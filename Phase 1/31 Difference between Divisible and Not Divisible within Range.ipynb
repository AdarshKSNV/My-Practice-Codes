   "source": [
    "// Calculate the differenece of sum of numbers that are divisible and not divisible by 6 in the range of first 30  numbers"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "id": "44aa2e70",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "-285\n"
     ]
    }
   ],
   "source": [
    "div=0\n",
    "notdiv=0\n",
    "+\n",
    "for i in range(1,31):\n",
    "    if(i%6==0):\n",
    "        div+=i\n",
    "    else:\n",
    "        notdiv+=i\n",
    "diff= div-notdiv\n",
    "print(diff)"
   ]
