    "def printThis(i,n):\n",
    "    if i>=n:\n",
    "        print(\"Base condition got hit\")\n",
    "        return \n",
    "    print(\"Ending line\",i)\n",
    "    for j in range(1,i):\n",
    "        print(j)\n",
    "    printThis(i+1,n)\n",
    "    print(\"Starting line\",i)\n",
    "printThis(1,8)"
